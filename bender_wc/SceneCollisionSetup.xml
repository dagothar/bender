<CollisionSetup>
	<Exclude>
		<FramePair first="Stelaz" second="Blat"/>
		<FramePair first="Blat" second="UR1.Base"/>
		<FramePair first="Blat" second="UR2.Base"/>
		<FramePair first="Blat" second="UR1_ghost.Base"/>
